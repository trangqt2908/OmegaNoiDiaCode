@model IEnumerable<Advertisement>

<div class="widget text-11 widget_text">
    <h3>
        Xem thêm
    </h3>
    <div class="presenting-sponsors">
       
        @foreach (var item in Model)
        {
            if (!String.IsNullOrEmpty(item.Media))
            {
                <a href="@item.Link" target="_blank"><img src="@item.Media" alt="@item.Title" width="360" height="100" /></a>
            }
        }
    </div>
</div>

