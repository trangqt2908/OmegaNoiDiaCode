@model IEnumerable<Advertisement>


@foreach (var item in Model)
{
    if (!String.IsNullOrEmpty(item.Media))
    {
        if (item.AdvertisementPosition.UID.Equals("bg-big"))
        {
            @item.Media
        }
        else
        {
            <img src="@item.Media" alt="@item.Title" />
        }
    }
}
