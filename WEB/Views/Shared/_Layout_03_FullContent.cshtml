<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <link rel="Shortcut Icon" href="/favicon.ico" type="image/x-icon" />
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />

    <title>
        @TempData["Title"]
    </title>
    <meta name="description" content="@TempData["Description"]" />
    <meta name="keywords" content="@TempData["Keywords"]" />

    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="~/Content/themes/web/css/magnific-popup.css" rel="stylesheet" />
    <link href="~/Content/themes/web/css/bootstrap.min.css" rel="stylesheet">
    <!-- Animate CSS -->
    <link href="~/Content/themes/web/css/animate.css" rel="stylesheet">
    <!-- Venobox CSS -->
    <link href="~/Content/themes/web/css/venobox.css" rel="stylesheet">
    <!-- Font-awesome -->
    <link rel="stylesheet" href="~/Content/themes/web/fonts/font-awesome/css/font-awesome.min.css">
    <!-- Flaticon -->
    <link rel="stylesheet" href="~/Content/themes/web/fonts/flaticon/css/flaticon.css">
    <!-- Google font-->
    <link href="https://fonts.googleapis.com/css?family=Poppins:300,400,400i,500,600,700,800,900" rel="stylesheet">
    <!-- Main CSS -->
    <link href="~/Content/themes/web/css/style.css" rel="stylesheet">

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <link href="~/Content/paging.css" rel="stylesheet" />
    <link href="~/Content/themes/web/css/responsive-accordion.css" rel="stylesheet" />
    <script src="~/Content/themes/web/js/jquery-3.3.1.min.js"></script>

    @*@Scripts.Render("~/bundles/modernizr")*@
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
    @RenderSection("scripts", required: false)

</head>
<body>

    <!--Preload-->
    <div class="preloader">
        <div class="preloader_image">
            <div class="sk-double-bounce">
                <div class="sk-child sk-double-bounce1"></div>
                <div class="sk-child sk-double-bounce2"></div>
            </div>
        </div>
    </div>

    @Html.Action("_Main", "Navigation", new { key = "main", area = "" })
    @RenderBody()
    <!-- end nav -->

    
    <footer id="footer">
        @Html.Action("_Menu_Footer", "Navigation", new { key = "footer", area = "" })


        <div class="footer-bottom">
            <div class="container">
                <div class="row">
                    <div class="col-sm-12 col-md-6">
                        <div class="copyright">
                            <p>Copyright &copy; @DateTime.Now.Year Aexcursions. All rights reserved.</p>
                        </div>
                    </div>
                    <div class="col-sm-12 col-md-6">
                        <ul class="social-icons pull-right">
                            <li><a href="#" target="_blank"><i class="fa fa-facebook"></i></a></li>
                            <li><a href="#" target="_blank"><i class="fa fa-twitter"></i></a></li>
                            <li><a href="#" target="_blank"><i class="fa fa-linkedin"></i></a></li>
                            <li><a href="#" target="_blank"><i class="fa fa-instagram"></i></a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <!-- end footer -->
    <!-- Bact to top -->
    <div class="back-top">
        <a href="#"><i class="fa fa-angle-up"></i></a>
    </div>

    <!-- jQuery -->
    <script src="~/Content/themes/web/js/popper.min.js"></script>
    <script src="~/Content/themes/web/js/bootstrap.min.js"></script>
    <script src="~/Content/themes/web/js/jquery.easing.min.js"></script>
    <script src="~/Content/themes/web/js/wow.js"></script>
    <script src="~/Content/themes/web/js/venobox.min.js"></script>
    <script src="~/Content/themes/web/js/SmoothScroll.js"></script>
    <script src="~/Content/themes/web/js/jquery.filterizr.min.js"></script>
    <script src="~/Content/themes/web/js/sticky-kit.min.js"></script>
    @*<script src="~/Content/themes/web/mail/js/form-validator.min.js"></script>
        <script src="~/Content/themes/web/mail/js/contact-form-script.js"></script>*@
    <script src="~/Content/themes/web/js/script.js"></script>
    <script src="/Content/themes/web/js/jquery.magnific-popup.js"></script>


    <script src="~/Content/themes/web/js/responsive-accordion.min.js" type="text/javascript"></script>
    <script src="~/Content/themes/web/js/SmoothScroll.js" type="text/javascript"></script>

</body>
</html>